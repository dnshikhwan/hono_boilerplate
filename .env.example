# Application
VERSION=1.0.0
APP_NAME=Hono + Bun Boilerplate API

# Server
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/honoapp

# JWT
JWT_SECRET=your-super-secret-jwt-key-here-must-be-at-least-32-characters-long
JWT_REFRESH_SECRET=your-super-secret-refresh-key-here-must-be-at-least-32-characters-long
JWT_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Security
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info